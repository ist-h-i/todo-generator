{
  "mcpServers": {
    "filesystem": {
      "command": "npx.cmd",
      "args": [
        "--yes",
        "@modelcontextprotocol/server-filesystem",
        "${workspaceFolder}/.."
      ],
      "disabledTools": []
    },
    "memory": {
      "command": "npx.cmd",
      "args": [
        "--yes",
        "@modelcontextprotocol/server-memory"
      ],
      "disabledTools": []
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "disabledTools": []
    },
    "puppeteer": {
      "command": "npx.cmd",
      "args": [
        "--yes",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "disabledTools": []
    },
    "sequential-thinking": {
      "command": "npx.cmd",
      "args": [
        "--yes",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "disabledTools": []
    },
    "time": {
      "command": "uvx",
      "args": [
        "mcp-server-time"
      ],
      "timeoutSeconds": 120,
      "disabledTools": []
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--python",
        "3.11",
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "codex",
        "--project",
        "${workspaceFolder}"
      ],
      "env": {
        "PYTHONUTF8": "1",
        "PYTHONIOENCODING": "UTF-8"
      },
      "timeoutSeconds": 180,
      "disabledTools": []
    },
    "mcp-playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "env": {},
      "disabledTools": []
    }
  }
}
